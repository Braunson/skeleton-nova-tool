{
    "name": "spatie/nova-tail-tool",
    "description": "A Laravel Nova tool to display the application log.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../laravel-nova"
        }
    ],
    "homepage": "https://github.com/spatie/nova-tail-tool",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TailTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TailTool\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\TailTool\\TailToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
